@{
    ViewData["Title"] = "Products";
}
@model IQueryable<Product>

@foreach (Product p in Model)
{
    <h2>@p.Name</h2>
    <ul>
        <li>@p.Price</li>
        <li>@p.PrimaryColor</li>
        <li>@p.Year</li>
    </ul>
}

<a asp-controller="Home" asp-action="Browse" asp-route-pageNum="1">1</a>
<a asp-controller="Home" asp-action="Browse" asp-route-pageNum="2">2</a>
<a asp-controller="Home" asp-action="Browse" asp-route-pageNum="3">3</a>